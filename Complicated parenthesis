// calculator.test.js

const calculator = new Calculator();

test('should handle parentheses correctly', () => {
  calculator.addChar('(');
  calculator.addChar('3');
  calculator.addChar('+');
  calculator.addChar('5');
  calculator.addChar(')');
  calculator.addChar('Ã—');
  calculator.addChar('2');
  calculator.calculate();
  expect(calculator.resultElement.textContent).toBe('16');
});
